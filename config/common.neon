parameters:
    site_name: 'Simple Gallery'

application:
	errorPresenter: Error
	mapping:
		*: App\Modules\*\Presenters\*Presenter


session:
	expiration: 14 days


di:
	export:
		parameters: no
		tags: no


services:
	- App\Router\RouterFactory::createRouter
	- App\Services\ParameterService(%appDir%, %site_name%)
	- App\Models\TagManager
